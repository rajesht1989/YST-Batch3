//  4 number from user identify the 3nd biggest number print on consol
#include<stdio.h>
int getNumberFromUser()
{
    int v;
    printf("enter the number");
    scanf("%d",&v);
    return v;
}
int biggestNumber(int a,int b,int c,int d)
{
    if(a>b&&a>c&&a>d)
    {
    return a;
    }
    else if(b>c&&b>d)
    {
        return b;
    }
    else if (c>d)
    {
        return c;
    }
    else 
    {
        return d;
    }
}
int thirdBiggestNumber(int a,int b,int c,int d)
    {
        int bNumber=biggestNumber(a,b,c,d);
       
            if(bNumber==a&&b)
            {
            return biggestNumber (0,0,c,d);
        }
        else (bNumber==c&&d);
        {
            return biggestNumber (a,b,0,0);
        }
    }
        

   
    void main()
    {
    int a= getNumberFromUser();
    int b= getNumberFromUser();
    int c= getNumberFromUser();
    int d= getNumberFromUser();
    int m=thirdBiggestNumber(a,b,c,d);
    printf("third biggest number is%d",m);
    }
