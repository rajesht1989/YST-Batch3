// get number from user identify  cylinder volume
//v=Ï€r^2h
#include<stdio.h>
int getNumberFromUser()
{
    int m; 
    printf("Enter a number");
    scanf("%d",&m);
    return m;
}
 float cylinderVolume(float r,float h)
{
    return 3.14*(r*r)*h;
}
void main() 
{
    int r=getNumberFromUser();
   
    int h=getNumberFromUser();
    float v=cylinderVolume(r,h);
    
}
