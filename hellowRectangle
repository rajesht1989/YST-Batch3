//get number from user print the rectangle in console
#include<stdio.h>
int getNumberFromUser()
{
    int v;
    printf("enter a number");
    scanf("%d",&v);
    return v;
}
int rectangle (int n)
{
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(i==0||j==0||i==n-1||j==n-1)
            {
                printf("*\t");
                
            }
            else
{
    printf("\t");

}

        }
        printf("\n");
    }
}
void main()
{
    int n= getNumberFromUser();
    rectangle(n);
}
