    return value;
}
int Biggest (int a,int b,int c,int d)
{
    if(a>b)
    {
        if(a>c)
        {
            if(a>d)
            {
                printf("Biggest no is%d",a);
            }
                    else
{
    printf("Biggest no is%d",d);
    
}
}
}
else
if(b>c)
