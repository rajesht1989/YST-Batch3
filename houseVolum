//get number from user identify the volume of house
#include <stdio.h>

int getNumberFromUser()
{
    int volume;
    printf("Enter the Number");
    scanf("%d", &volume);
    return volume;
}
int areaOfHouse(int w,int h,int l)
{
    return (w*h*l);
}
   void main ()
   {
    int w = getNumberFromUser();
    int h = getNumberFromUser();
    int l = getNumberFromUser();
    int v = areaOfHouse (w,h,l);
    printf("area of house volum%d",v);
   
}
