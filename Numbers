//REQUIREMENT:GET NUMBER FROM USER AND PRINT NUMBER SPELLING ON CONSOLE
int getNumberFromUser(){
    int r;
    printf("Enter A Numbers");
    scanf("%d",&r);
    return r;
}
void printWord(int a){
    if(a==1){
        printf("ONE");
    }
    else if(a==2){
        printf("TWO");
    }
    else if(a==3){
        printf("THREE");
    }
    else if(a==4){
        printf("FOUR");
    }
    else if(a==5){
        printf("FIVE");
    }
    else if(a==6){
        printf("SIX");
    }
    else{
        printf("CAN Not BE IDENTIFY SORRY");
    }

}
#include<stdio.h>
void main(){
printWord(getNumberFromUser());
}
